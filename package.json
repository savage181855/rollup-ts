{
  "name": "rollup-ts",
  "version": "1.0.0",
  "description": "rollup打包ts模板",
  "main": "dist/index.cjs",
  "module": "./dist/index.mjs",
  "miniprogram": "dist",
  "files": [
    "dist/index.cjs",
    "dist/index.mjs",
    "dist/main.d.ts",
    "dist/index.cjs.map",
    "dist/index.mjs.map"
  ],
  "types": "./dist/main.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "scripts": {
    "dev": "cross-env mode=dev nodemon rollup.config.js",
    "build": "cross-env mode=pro rollup -c rollup.config.js",
    "prepush": "npm run build",
    "push": "npm publish --access public",
    "lint": "eslint .",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "savage-rollup-config": "^0.0.13"
  },
  "dependencies": {}
}